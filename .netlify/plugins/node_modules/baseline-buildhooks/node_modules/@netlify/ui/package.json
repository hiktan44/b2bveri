{
  "name": "@netlify/ui",
  "version": "0.0.20",
  "type": "module",
  "dependencies": {
    "clsx": "^2.1.0",
    "@netlify/netlify-design-tokens": "^6.0.0",
    "@netlify/tailwind-config": "^2.0.0",
    "history": "^4.7.2",
    "downshift": "^9.0.0",
    "@tippyjs/react": "^4.2.6",
    "lodash": "^4.17.21",
    "react-to-text": "2.0.1",
    "tippy.js": "6.3.7"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "author": "Netlify",
  "description": "A shared UI component library for Netlify",
  "homepage": "https://github.com/netlify/netlify-react-ui/tree/main/libs/ui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/netlify-react-ui.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "netlify",
    "shared"
  ],
  "exports": {
    "./base.css": {
      "import": "./base.css",
      "require": "./base.css"
    },
    "./styles.css": {
      "import": "./styles.css",
      "require": "./styles.css"
    },
    "./themes.css": {
      "import": "./themes.css",
      "require": "./themes.css"
    },
    "./helpers/cn": {
      "import": "./helpers/cn.js",
      "require": "./helpers/cn.cjs",
      "types": "./helpers/cn.d.ts"
    },
    "./helpers/remToPxNumber": {
      "import": "./helpers/remToPxNumber.js",
      "require": "./helpers/remToPxNumber.cjs",
      "types": "./helpers/remToPxNumber.d.ts"
    },
    "./helpers/format": {
      "import": "./helpers/format.js",
      "require": "./helpers/format.cjs",
      "types": "./helpers/format.d.ts"
    },
    "./helpers/organizations": {
      "import": "./helpers/organizations.js",
      "require": "./helpers/organizations.cjs",
      "types": "./helpers/organizations.d.ts"
    },
    "./helpers/placeholderColors": {
      "import": "./helpers/placeholderColors.js",
      "require": "./helpers/placeholderColors.cjs",
      "types": "./helpers/placeholderColors.d.ts"
    },
    "./helpers/plans": {
      "import": "./helpers/plans.js",
      "require": "./helpers/plans.cjs",
      "types": "./helpers/plans.d.ts"
    },
    "./helpers/path": {
      "import": "./helpers/path.js",
      "require": "./helpers/path.cjs",
      "types": "./helpers/path.d.ts"
    },
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  }
}
